olor: white;\r\n}\r\n\r\n.wrapper{\r\n    width: 100%;\r\n    height: 100vh;\r\n}\r\n\r\n.mainImage{\r\n    display: flex;\r\n    align-self: center;\r\n    justify-self: center;\r\n\r\n    .covidHalf{\r\n        display: none;\r\n        width: 225px;\r\n        height: 450px;\r\n\r\n        @media only screen and (max-width: 1100px){\r\n            display: block;\r\n        }\r\n    }\r\n\r\n    .covidFull{\r\n        width: 450px;\r\n        @media only screen and (max-width: 1100px){\r\n            display: none;\r\n        } \r\n    }\r\n\r\n    @media only screen and (max-width: 1100px){\r\n        justify-self: flex-end;\r\n    }\r\n}\r\n\r\n.left{\r\n    align-items: center;\r\n    justify-items: flex-start;\r\n    margin-left: 3rem;\r\n\r\n    p{\r\n        font-size: 3rem;\r\n        font-family: 'Oswald', sans-serif;\r\n        font-weight: 400;\r\n        letter-spacing: .8rem;\r\n\r\n        @media only screen and (max-width: 830px){\r\n            font-size: 2.5rem;\r\n        }\r\n    }\r\n\r\n    #casses{\r\n        align-self: flex-end;\r\n        margin-bottom: 3rem;\r\n    }\r\n\r\n    #viewCountry{\r\n        margin-top: 10rem;\r\n        font-size: 2rem;\r\n        align-self: flex-end;\r\n    }\r\n\r\n    #deaths{\r\n        color: #D31311;\r\n    }\r\n\r\n    #recovered{\r\n        color: #15A83C;\r\n        align-self: flex-start;\r\n        margin-top: 4rem;\r\n    }\r\n}\r\n\r\n.right{\r\n    align-items: center;\r\n    justify-items: center;\r\n\r\n    p{\r\n        align-self: flex-end;\r\n        color: #D31311;\r\n        font-size: 10rem;\r\n        font-family: 'Oswald', sans-serif;\r\n        font-weight: 500;\r\n        letter-spacing: 3rem;\r\n        line-height: 12rem;\r\n        padding-left: 35%;\r\n\r\n        @media only screen and (max-width: 1100px){\r\n            display: flex;\r\n            justify-content: center;\r\n            padding-left: 0; \r\n            height: 100vh;\r\n            width: 100%;\r\n        }\r\n\r\n        @media only screen and (max-width: 1000px){\r\n            font-size: 7rem;\r\n        }\r\n\r\n        &:last-child{\r\n            align-self: flex-start;\r\n        }\r\n\r\n    }\r\n\r\n    .logo{\r\n        padding-left: 30%;\r\n        img{\r\n            width: 180px;\r\n        }\r\n\r\n        @media only screen and (max-width: 1100px){\r\n            display: flex;\r\n            position: absolute;\r\n            padding-left: 0;\r\n            left: 10%;\r\n            right: 0;\r\n            justify-content: center;\r\n        }\r\n\r\n        @media only screen and (max-width: 830px){\r\n            justify-content: center;\r\n            align-self: flex-end;\r\n        }\r\n    }\r\n\r\n    @media only screen and (max-width: 1100px){\r\n        flex-direction: row;\r\n        align-self: flex-start;\r\n    }\r\n}","styles\\_grid.scss":".wrapper{\r\n    display: grid;\r\n    grid-template-columns: minmax(100px, 1fr) minmax(min-content, 1410px) minmax(100px, 1fr);\r\n\r\n    @media only screen and (max-width:1300px ){\r\n        grid-template-columns: minmax(60px, 1fr) minmax(min-content, 1410px) minmax(60px, 1fr);\r\n    }\r\n\r\n    @media only screen and (max-width:1200px ){\r\n        grid-template-columns: minmax(40px, 1fr) minmax(min-content, 1410px) minmax(40px, 1fr);\r\n    }\r\n\r\n    @media only screen and (max-width:1100px ){\r\n        grid-template-columns: minmax(40px, 1fr) minmax(min-content, 1410px);\r\n    }\r\n}\r\n\r\n.content{\r\n    grid-column: 2 / 3;\r\n\r\n    display: grid;\r\n    grid-template-columns: 1fr 1fr;\r\n\r\n    @media only screen and (max-width:780px ){\r\n        grid-template-columns: 1fr .5fr\r\n    }\r\n}\r\n\r\n#canvas{\r\n    grid-column: 1 / -1;\r\n    grid-row: 1 / -1;\r\n}\r\n\r\n.mainImage{\r\n    grid-column: 1 / -1;\r\n    grid-row: 1 / -1;\r\n}\r\n\r\n.left{\r\n    grid-column: 1;\r\n    grid-row: 1;\r\n\r\n    display: grid;\r\n    grid-template-rows: 1fr 1fr 1fr;\r\n\r\n    #casses{\r\n        grid-column: 1;\r\n        grid-row: 1 / 2;\r\n    }\r\n    #viewCountry{\r\n        grid-column: 1;\r\n        grid-row: 1 / 2;\r\n    }\r\n\r\n    #deaths{\r\n        grid-row: 2 / 3;\r\n    }\r\n\r\n    #recovered{\r\n        grid-row: 3 / 4;\r\n    }\r\n}\r\n\r\n.right{\r\n    grid-column: 2;\r\n    grid-row: 1;\r\n\r\n    display: grid;\r\n    grid-template-rows: 1fr 1fr 1fr;\r\n\r\n    @media only screen and (max-width: 1100px){\r\n        grid-column: 1 / -1;\r\n        display: flex; \r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"17e9db8a4871d2b96f91a413be5aac7e","cacheData":{"env":{}}}