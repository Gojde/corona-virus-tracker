   #recovered{\r\n        color: #15A83C;\r\n        border-left: 1.5px solid #15A83C;\r\n        align-self: flex-start;\r\n        margin-top: 4rem;\r\n    }\r\n}\r\n\r\n.right{\r\n    align-items: center;\r\n    justify-items: center;\r\n\r\n    p{\r\n        align-self: flex-end;\r\n        color: #D31311;\r\n        font-size: 10rem;\r\n        font-family: 'Oswald', sans-serif;\r\n        font-weight: 500;\r\n        letter-spacing: 3rem;\r\n        line-height: 12rem;\r\n        padding-left: 35%;\r\n\r\n        @media only screen and (max-width: 1100px){\r\n            display: flex;\r\n            justify-content: center;\r\n            padding-left: 0; \r\n            height: 100vh;\r\n            width: 100%;\r\n        }\r\n\r\n        @media only screen and (max-width: 1000px){\r\n            font-size: 7rem;\r\n        }\r\n\r\n        @media only screen and (max-width: 700px){\r\n            font-size: 5rem;\r\n        }\r\n\r\n        @media only screen and (max-width: 600px){\r\n            font-size: 4rem;\r\n        }\r\n\r\n        @media only screen and (max-width: 500px){\r\n            font-size: 3rem;\r\n        }\r\n\r\n        &:last-child{\r\n            align-self: flex-start;\r\n        }\r\n\r\n    }\r\n\r\n    .logo{\r\n        padding-left: 30%;\r\n        img{\r\n            width: 180px;\r\n        }\r\n\r\n        @media only screen and (max-width: 1100px){\r\n            display: flex;\r\n            position: absolute;\r\n            padding-left: 0;\r\n            left: 10%;\r\n            right: 0;\r\n            justify-content: center;\r\n        }\r\n\r\n        @media only screen and (max-width: 830px){\r\n            justify-content: center;\r\n            align-self: flex-end;\r\n            padding-bottom: 2rem;\r\n        }\r\n\r\n        @media only screen and (max-width: 500px){\r\n            img{\r\n                width: 150px;\r\n            }\r\n        }\r\n\r\n    }\r\n\r\n    @media only screen and (max-width: 1100px){\r\n        flex-direction: row;\r\n        align-self: flex-start;\r\n    }\r\n}","styles\\_grid.scss":".wrapper{\r\n    display: grid;\r\n    grid-template-columns: minmax(100px, 1fr) minmax(min-content, 1410px) minmax(100px, 1fr);\r\n\r\n    @media only screen and (max-width:1300px ){\r\n        grid-template-columns: minmax(60px, 1fr) minmax(min-content, 1410px) minmax(60px, 1fr);\r\n    }\r\n\r\n    @media only screen and (max-width:1200px ){\r\n        grid-template-columns: minmax(40px, 1fr) minmax(min-content, 1410px) minmax(40px, 1fr);\r\n    }\r\n\r\n    @media only screen and (max-width:1100px ){\r\n        grid-template-columns: minmax(40px, 1fr) minmax(min-content, 1410px);\r\n    }\r\n}\r\n\r\n.content{\r\n    grid-column: 2 / 3;\r\n\r\n    display: grid;\r\n    grid-template-columns: 1fr 1fr;\r\n\r\n    @media only screen and (max-width:780px ){\r\n        grid-template-columns: 1fr .5fr;\r\n    }\r\n\r\n    @media only screen and (max-width:550px ){\r\n        grid-template-columns: 1fr 1.5fr;\r\n    }\r\n}\r\n\r\n#canvas{\r\n    grid-column: 1 / -1;\r\n    grid-row: 1 / -1;\r\n}\r\n\r\n.mainImage{\r\n    grid-column: 1 / -1;\r\n    grid-row: 1 / -1;\r\n}\r\n\r\n.left{\r\n    grid-column: 1;\r\n    grid-row: 1;\r\n\r\n    display: grid;\r\n    grid-template-rows: 1fr 1fr 1fr;\r\n\r\n    #casses{\r\n        grid-column: 1;\r\n        grid-row: 1 / 2;\r\n    }\r\n    #viewCountry{\r\n        grid-column: 1;\r\n        grid-row: 1 / 2;\r\n    }\r\n\r\n    #deaths{\r\n        grid-row: 2 / 3;\r\n    }\r\n\r\n    #recovered{\r\n        grid-row: 3 / 4;\r\n    }\r\n}\r\n\r\n.right{\r\n    grid-column: 2;\r\n    grid-row: 1;\r\n\r\n    display: grid;\r\n    grid-template-rows: 1fr 1fr 1fr;\r\n\r\n    @media only screen and (max-width: 1100px){\r\n        grid-column: 1 / -1;\r\n        display: flex; \r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"023f2c72ec305b2bfde6fb5dfc5eddcf","cacheData":{"env":{}}}