-rows: 1fr 1fr 1fr;\r\n\r\n    #casses{\r\n        grid-column: 1;\r\n        grid-row: 1 / 2;\r\n    }\r\n    #viewCountry{\r\n        grid-column: 1;\r\n        grid-row: 1 / 2;\r\n    }\r\n\r\n    #deaths{\r\n        grid-row: 2 / 3;\r\n    }\r\n\r\n    #recovered{\r\n        grid-row: 3 / 4;\r\n    }\r\n}\r\n\r\n.right{\r\n    grid-column: 2;\r\n    grid-row: 1;\r\n\r\n    display: grid;\r\n    grid-template-rows: 1fr 1fr 1fr;\r\n\r\n    @media only screen and (max-width: 1100px){\r\n        grid-column: 1 / -1;\r\n        display: flex; \r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"d2c5c3f872c01d0001dec8586a2edca5","cacheData":{"env":{}}}